{
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "eslint.validate": [
        "javascript",
        "typescript",
        "typescriptreact",
        "javascriptreact"
    ],
    "files.eol": "\n",
    "tailwindCSS.includeLanguages": {
        "typescript": "javascript",
        "typescriptreact": "javascript"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "always"
    }
}